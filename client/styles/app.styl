coral = #E45F56
pistachio = #A3D39C
robinEgg = #7ACCC8
turquoise = #4AAAA5
slate = #35404F

user-select()
  -webkit-user-select none
  -moz-user-select none
  -ms-user-select none

body
  margin: 5px
  color: robinEgg
  user-select()
  background: slate
  // background-image: url("../assets/zenbg.png")
  text-align: center
  font-family: RobotoDraft, 'Helvetica Neue', Helvetica, Arial
  line-height: 1.4em

input:focus
  outline 3pt solid coral


p.instruct
  font-size: 10pt
  padding: 0
  margin: 0
  color: black

.toolbar
  height 10vh

.test
    border: 2px solid robinEgg
    height: 20vh
    color #ccc
    text-align: center
    padding: 20px
    margin-left: auto
.work
  border: 2px solid robinEgg
  color #ccc
  height 40vh
  padding: 5px

.container
    color #ccc
    height 30vh
    display table
.cell
  display table-cell
  width 33.333vw
  border 2px solid robinEgg
.teststring
  margin 0px auto 0 auto
  width 400px
  height 20px
.RR-dir
  // width 100px
.informativeText
  position absolute
  float left
  margin-left 150px
  width 200px
.ghost
  stroke gray
  fill gray

.textBox
  font-weight bold
  padding 2px
  border-radius 8px
  background-color coral
  border 2px solid coral

.textBox:focus {
  border 2px solid turquoise
  outline none
}

.paper-shadow
  box-shadow: 0 2px 5px 0 rgba(0, 0, 0, 0.26)

.paper-toolbar
  color: #fff
  background: turquoise

.app-bar, .navdrawer-container, main
  -webkit-transition: -webkit-transform 0.3s ease-out
  transition: transform 0.3s ease-out

.promote-layer
  -webkit-backface-visibility: hidden
  backface-visibility: hidden

.app-bar-container
  display: -webkit-flex
  display: -ms-flexbox
  display: flex
  width: 100%
  height: 64px
  position: relative
  -webkit-flex-direction: row
  -ms-flex-direction: row
  flex-direction: row
  margin: 0 auto

.app-bar
  display: block
  width: 100%
  position: fixed
  top: 0
  left: 0
  overflow: hidden
  z-index: 1
  margin-bottom: 40px

.app-bar .logo
  -webkit-flex: 1
  -ms-flex: 1
  flex: 1
  font-size: 1.3em
  margin: 0 16px
  padding: 0
  color: #fefefe
  float: none
  max-width: none
  font-weight: 400
  line-height: 64px
#demoCustom mark
  padding: 0
  margin: -1px -4px
  border-radius: 0.5em
  border: 1px solid pink
  text-align: center
